[Unit]
Description=CcspSnmppa service

After= cosastart-rem.service snmpd.service
Requires=snmpd.service

[Service]
Type=forking
WorkingDirectory=/usr/ccsp/snmp
Environment="LOG4C_RCPATH=/etc"
Environment="MIBS=ALL"
ExecStart=/usr/bin/snmp_subagent
RemainAfterExit=yes
Restart=always
StandardOutput=syslog

[Install]
WantedBy=multi-user.target
